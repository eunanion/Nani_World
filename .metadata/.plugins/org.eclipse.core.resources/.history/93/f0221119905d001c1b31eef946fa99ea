package game.modifier;

public class Main02 {

	public static void main(String[] args) {
		Warrior p = new Warrior("아이즈원");
		
		if (p.total > 70) {
		    p.levelUp();
		    System.out.println(p.getLv()+"레벨업 ㅊㅋㅋ");
		}
		p.getInfo();
		p.hunt();
		p.hunt();
		p.hunt();
		p.getInfo();
		p.doubleAttack();
		p.doubleAttack();
		p.howl();
		p.levelUp();
		p.getInfo();
		

	}

}
