
public class Ch3_1 {
	public static void main(String[] args) {
		
		int x = 2;
		int y = 5;
		char c = 'A';
		
		System.out.println( y >= 5 || x < 0 && x > 2);
		//True
		System.out.println(y += 10 - x++);
		//12
		System.out.println(x += 2);
		//5
		System.out.println(!('A' <= c && c <= "Z"));
		//True
	}

}
