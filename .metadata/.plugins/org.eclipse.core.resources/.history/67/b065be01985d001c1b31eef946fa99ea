package game.modifier;

public class Main02 {

	public static void main(String[] args) {
		Warrior p = new Warrior("아이즈원");
		
		
		p.getInfo();
		p.hunt();
		p.hunt();
		p.hunt();
		p.hunt();
		p.getInfo();
		p.hunt();
		p.hunt();
		p.hunt();
		p.hunt();
		p.hunt();
		p.getInfo();
		p.hunt();
		p.hunt();
		p.hunt();
		p.hunt();
		p.getInfo();
		p.hunt();
		if (p.getExp() > 70) {
		    p.levelUp();
		}
		p.hunt();
		p.hunt();
		if (p.getExp() > 70) {
		    p.levelUp();
		}
		p.hunt();
		p.hunt();
		p.hunt();

		p.getInfo();
		p.doubleAttack();
		p.doubleAttack();
		p.howl();
		p.levelUp();
		p.getInfo();
		

	}

}
