package constructure;

public class BasketballPlayer {

	public int height;
	public int jumpHeight;
	
	public BasketballPlayer(int h, int j) {
		
		height=h;
		jumpHeight=j;
		System.out.println("생선된 선수 키 : "+height+", 점프력: "+
		jumpHeight+" 입니다");
	}
	
	public void dunkShoot() {
		switch (height+jumpHeight) {
		case 300 : System.out.println("덩크슛 성공!");
		default: System.out.println("아쉽네요");
		} 
		
	}
	}
	